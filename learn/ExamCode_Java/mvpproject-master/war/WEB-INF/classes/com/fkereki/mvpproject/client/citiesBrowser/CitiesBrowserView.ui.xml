<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<u:UiBinder xmlns:u='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
  <g:HTMLPanel>
     <h1>CitiesBrowser</h1>
     <table>
      <tr>
        <td>Country:</td>
        <td><g:ListBox u:field="countryCode"/></td>
      </tr>
      <tr>
        <td>State:</td>
        <td><g:ListBox u:field="stateCode"/></td>
      </tr>
    </table>
    <hr />
    <g:Button u:field="firstButton"/><g:Button u:field="previousButton"/><g:Button u:field="nextButton"/>
    <hr />
    <g:FlexTable u:field="cg"/>
  </g:HTMLPanel>
</u:UiBinder>