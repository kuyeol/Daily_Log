<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<u:UiBinder xmlns:u='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui' 
  xmlns:h='urn:import:com.fkereki.mvpproject.client'>
  
  <g:FormPanel u:field="downloadForm">
    <g:HTMLPanel>
      <h1>FileDownloadView</h1>
      <table>
        <tr>
          <td>
            <g:Label text='1st parameter:' />
          </td>
          <td>
            <g:TextBox u:field="parameter1" name="parameter1" />
          </td>
        </tr>
        <tr>
          <td>
            <g:Label text='2nd parameter:' />
          </td>
          <td>
            <g:TextBox u:field="parameter2" name="parameter2" />
          </td>
        </tr>
        <tr>
          <td>
            <g:Label text='3rd parameter:' />
          </td>
          <td>
            <g:TextBox u:field="parameter3" name="parameter3" />
          </td>
        </tr>
        <tr>
          <td></td>
          <td>
            <g:Button u:field="downloadButton" text='Get it' />
            <g:Anchor u:field="downloadLink" text='Or get it' />
          </td>
        </tr>
      </table>
    </g:HTMLPanel>
  </g:FormPanel>
</u:UiBinder>